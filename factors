#!/usr/bin/python3
import sys
import math

def prime_factors(n):
    factors = []
    while n % 2 == 0:
        factors.append(2)
        n = n // 2
    for i in range(3, int(math.sqrt(n)) + 1, 2):
        while n % i == 0:
            factors.append(i)
            n = n // i
    if n > 2:
        factors.append(n)
    return factors

def format_output(number, factors):
    return f"{number}={'*'.join(map(str, factors))}"

def calculate_factors(input_file):
    with open(input_file, 'r') as file:
        numbers = file.readlines()

    results = []
    for number in numbers:
        number = int(number.strip())
        factors = prime_factors(number)
        results.append(format_output(number, factors))

    return results

if __name__ == '__main__':
    input_file = 'tests/test00'
    results = calculate_factors(input_file)

    for result in results:
        print(result)
